{
  "manifest_version": 2,
  "name": "LeetCodeElo",
  "version": "1.0",

  "description": "Replaces Frequency with Elo Rating on LeetCode",

  "permissions": [
    "*://zerotrac.github.io/*",
    "activeTab",
    "webRequest"
  ],

  "content_scripts": [
    {
      "matches": ["*://*.leetcode.com/*"],
      "js": ["index.js"]
    }
  ]
}
